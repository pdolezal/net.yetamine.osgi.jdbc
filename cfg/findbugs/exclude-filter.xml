<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Class name="net.yetamine.osgi.jdbc.DriverProvider" />

        <Or>
            <Method name="connection" />
        </Or>

        <Bug pattern="UC_USELESS_OBJECT" />
    </Match>

    <Match>
        <Class name="net.yetamine.osgi.jdbc.internal.DriverTracking$DriverTracker" />

        <Or>
            <Method name="addingService" />
            <Method name="modifiedService" />
        </Or>

        <Bug pattern="JLM_JSR166_UTILCONCURRENT_MONITORENTER" />
    </Match>
</FindBugsFilter>
